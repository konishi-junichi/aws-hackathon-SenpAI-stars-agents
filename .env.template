# AWS Configuration
# Replace with your AWS region (e.g., us-east-1, us-west-2, eu-west-1)
AWS_REGION=YOUR_AWS_REGION

# Replace with your AWS account ID (12-digit number)
AWS_ACCOUNT_ID=YOUR_AWS_ACCOUNT_ID

# Agent Configuration
# Customize the agent name for your deployment
AGENT_NAME=YOUR_AGENT_NAME
AWS_MEMORY_ID=YOUR_AWS_MEMORY_ID

# Agent endpoint name (usually matches agent name or use a descriptive name)
AGENT_ENDPOINT_NAME=YOUR_AGENT_ENDPOINT_NAME

# Agent runtime ID (will be generated after first deployment)
# Leave as placeholder initially, update after deployment
AGENT_RUNTIME_ID=YOUR_AGENT_RUNTIME_ID

# Agent runtime ARN (uses variable substitution from above values)
# This format should work for most cases, no need to change
AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:${AWS_REGION}:${AWS_ACCOUNT_ID}:runtime/${AGENT_RUNTIME_ID}

# Test Configuration
# Unique session ID for testing (can be any descriptive string)
RUNTIME_SESSION_ID=YOUR_TEST_SESSION_ID

# Qualifier for agent version (DEFAULT for latest, or specific version)
QUALIFIER=DEFAULT

# Example filled values (commented out):
# AWS_REGION=us-west-2
# AWS_ACCOUNT_ID=123456789012
# AGENT_NAME=my_custom_agent
# AGENT_ENDPOINT_NAME=my-custom-agent
# AGENT_RUNTIME_ID=my_custom_agent-abc123xyz
# RUNTIME_SESSION_ID=test-session-001